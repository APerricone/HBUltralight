{
    "harbour.extraIncludePaths": [
        "c:\\harbour\\include",
        "ultralight\\include\\",
        ".", ".."
    ],
    "files.associations": {
        "*.h": "c",
        "array": "cpp",
        "vector": "cpp",
        "xstring": "cpp"
    },
    "editor.detectIndentation": false,
    "editor.useTabStops": false,
    "editor.tabSize": 4,
    "editor.trimAutoWhitespace": true,
    "terminal.integrated.shell.windows": "C:\\Windows\\System32\\cmd.exe",
    "files.exclude": {
        "*.dll": true,
        "*.exe": true,
        "*.lib": true,
        "*.so": true,
        "*.a": true,
        "*.pdb": true,
        "*.7z": true,
    }
}
